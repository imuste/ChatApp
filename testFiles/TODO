tar -cvf a2_part1.tar.gz serverDriver.c server.c server.h README
provide comp112 a2_part1 a2_part1.tar.gz

#FINAL
tar -cvf a2.tar.gz serverDriver.c server.c server.h README
provide comp112 a2 a2.tar.gz


